{
	"extends": [
		"tslint:recommended",
		"tslint-react",
		"tslint-config-prettier"
	],
	"linterOptions": {
		"exclude": [
			"config/**/*.js",
			"node_modules/**/*.ts",
			"coverage/lcov-report/*.js"
		]
	},
	"rules": {
		"ordered-imports": [
			false
		],
		"object-literal-sort-keys": [
			false
		],
		"no-unused-expression": false,
		"interface-name": [true, "never-prefix"],
		"no-empty": false,
		"prefer-const": false,
		"one-variable-per-declaration": false,
		"member-ordering": false,
		"jsdoc-format": false,
		"array-type": false,
		"member-access": false,
		"no-empty-interface": false,
		"jsx-no-lambda": false,
		"no-console": false,
		"curly": false,
		"triple-equals": false,
		"no-string-throw": false,
		"variable-name": false
	}
}